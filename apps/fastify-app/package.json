{
  "name": "fastify-app",
  "version": "0.1.87",
  "type": "module",
  "private": true,
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "license": "MIT",
  "homepage": "https://github.com/belgattitude/workshop-node-sql-server",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belgattitude/workshop-node-sql-server.git",
    "directory": "examples/fastify-app"
  },
  "scripts": {
    "dev": "run clean && cross-env NODE_ENV=development tsx watch ./src/server.ts --watch-path=./src --clear-screen=false | pino-pretty",
    "build": "tsc",
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-watch": "vitest --ui",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "typecheck-no-paths": "tsc --project tsconfig.no-paths.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/fastify-app.eslintcache",
    "prettier-check": "yarn run --top-level prettier --check --cache --cache-location=\"../../.cache/prettier/fastify-app.prettiercache\" --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "prettier-fix": "yarn run --top-level prettier --write --cache --cache-location=\"../../.cache/prettier/fastify-app.prettiercache\" --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "fix-staged": "lint-staged --allow-empty"
  },
  "dependencies": {
    "@fastify/autoload": "6.2.0",
    "@fastify/sensible": "6.0.3",
    "@fastify/swagger": "9.4.2",
    "@fastify/swagger-ui": "5.2.2",
    "@fastify/type-provider-typebox": "5.1.0",
    "@flowblade/core": "0.2.8",
    "@flowblade/source-kysely": "0.13.12",
    "@httpx/assert": "0.15.2",
    "@httpx/dsn-parser": "1.8.6",
    "@httpx/exception": "3.0.12",
    "@httpx/memo-intl": "1.2.7",
    "@httpx/plain-object": "2.0.6",
    "@httpx/treeu": "0.4.12",
    "@sinclair/typebox": "0.34.31",
    "@workshop/db-sqlserver": "workspace:*",
    "close-with-grace": "2.2.0",
    "fastify": "5.2.1",
    "fastify-cli": "7.3.0",
    "fastify-plugin": "5.0.1",
    "kysely": "0.27.6",
    "pino": "9.6.0",
    "pino-pretty": "13.0.0",
    "tedious": "19.0.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "6.30.0",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/node": "22.13.13",
    "@typescript-eslint/eslint-plugin": "8.27.0",
    "@typescript-eslint/parser": "8.27.0",
    "@vitest/coverage-v8": "3.0.9",
    "@vitest/ui": "3.0.9",
    "cross-env": "7.0.3",
    "dotenv-expand": "12.0.1",
    "dotenv-flow": "4.1.0",
    "eslint": "8.57.1",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "tiny-glob": "0.2.9",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "vite": "6.2.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.9"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
